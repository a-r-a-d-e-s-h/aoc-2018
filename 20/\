from collections import defaultdict

class Room:
    all_rooms = {}
    def __init__(self, row, col):
        self.row = row
        self.col = col
        self.all_rooms[row, col] = self
        self.directions = []
        self.distance = 0

    @property
    def coord(self):
        return (row, col)

rooms = dict()
start = 0
rooms[0, 0] = start

start = (0, 0)
rooms[start] = 0

